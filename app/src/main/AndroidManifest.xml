<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.mybabyplayer">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_baby"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_baby"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NowPlaying"
            android:label="@string/act_playing_now" />
        <activity
            android:name=".DiscoverActivity"
            android:label="@string/act_discover" />
        <activity
            android:name=".PlaylistsActivity"
            android:label="@string/act_playlists" />
        <activity
            android:name=".LibraryActivity"
            android:label="@string/act_library" />
    </application>

</manifest>
